application {
  config {
    baseName Boutique
    packageName org.iqkv.boutique
    authenticationType jwt
    prodDatabaseType postgresql
    buildTool maven
    clientFramework vue
    enableTranslation true
    nativeLanguage en
    languages [en, ru, fr, it]
    skipClient false
    skipServer false
    testFrameworks  [cypress, cucumber]
  }
}